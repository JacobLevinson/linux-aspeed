&video0 {
	status = "okay";
	memory-region = <&video_engine_memory0>;
};

&espi0 {
	status = "okay";
	perif-dma-mode;
	oob-dma-mode;
	flash-dma-mode;
};

&lpc0_pcc {
        port-addr = <0x80>;
        dma-mode;
        A2600-15;
        rec-mode = <0x1>;
        port-addr-hbits-select = <0x1>;
        port-addr-xbits = <0x3>;

        status = "okay";
};

&jtag0 {
